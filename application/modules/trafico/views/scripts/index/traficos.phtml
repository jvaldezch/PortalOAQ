<div class="row-fluid">
    <div id="mm" class="easyui-menu" data-options="onClick:menuHandler" style="width: 220px;">
        <div data-options="name:'open'">Abrir</div>
        <div data-options="name:'soia'">Estatus SOIA</div>
        <div data-options="name:'documents'">Documentos completos</div>
        <div data-options="name:'cancel'">Cancelar Operación</div>
        <div data-options="name:'justification'">Justificar</div>
        <div data-options="name:'delete'">Eliminar</div>
        <div class="menu-sep"></div>
        <div data-options="name:'status', status: 1, iconCls:'icon-status-1'">Programado</div>
        <div data-options="name:'status', status: 7, iconCls:'icon-status-7'">En captura</div>
        <div data-options="name:'status', status: 5, iconCls:'icon-status-5'">Proforma</div>
        <div data-options="name:'status', status: 2, iconCls:'icon-status-2'">Pagado</div>
